{
    "version": 2,
    "builds":[
        {
            "src": "myblog/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxsize":"15mb","runtime":"python3.10"}
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config":{
            "distDir": "staticfiles_build"
        }
      }
    ],
    "routes": [
        {
            "src":"/static(.*)",
            "dest": "/static/1$"            
        },
        {
            "src": "/(.*)",
            "dest": "myblog/wsgi.py"

        }
    ]


}